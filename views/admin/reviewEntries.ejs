<hr class="uk-divider-icon" />
<div
  class="
    uk-child-width-1-1
    uk-child-width-1-3@s
    uk-grid-small
    uk-margin-large-top
    uk-margin-large-bottom
  "
  uk-grid
>
  <% if(reviewEntries.length > 0) { %> <% reviewEntries.forEach((reviewEntry)=>
  { %>
  <div>
    <a href="#" class="noSelect"
      ><div class="uk-card uk-card-hover uk-card-default">
        <div class="uk-card-media-top">
          <div class="uk-clearfix uk-position-absolute">
            <span
              class="
                uk-label
                uk-float-left
                uk-label-danger
                uk-text-medium
                uk-text-bold
              "
              ><%- reviewEntry.typeOfPlace %></span
            >
            <span
              class="
                uk-label
                uk-float-left
                uk-label-primary
                uk-text-medium
                uk-text-bold
              "
              ><%- reviewEntry.bookingStatus %></span
            >
            <span
              class="
                uk-label
                uk-float-right
                uk-label-success
                uk-text-medium
                uk-text-bold
              "
              ><%- reviewEntry.rating +" " + "/" + " " + 5%></span
            >
          </div>
          <img src="<%- reviewEntry.image %>" alt="<%- reviewEntry.image %>" />
        </div></div
    ></a>
  </div>
  <% }) %> <% } else { %>
  <p class="uk-text-center uk-text-large uk-text-warning">No Entries</p>
  <% } %>
</div>
