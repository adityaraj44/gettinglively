    >
  </li>
</ul>

<!-- body -->
<div
  class="
    uk-section uk-section-muted uk-section-statistics uk-section-large
    borderBottom
  "
>
  <div class="uk-container">
    <h4 class="uk-text-uppercase uk-text-center uk-margin-top-large tagline1">
      Entries awaiting approval
    </h4>
    <hr class="uk-divider-icon" />
    <div
      class="
        uk-child-width-1-1
        uk-child-width-1-3@s
        uk-grid-small
        uk-margin-large-top
        uk-margin-large-bottom
      "
      uk-grid
    >
      <% if(reviewEntries.length > 0) { %> <%
      reviewEntries.forEach((reviewEntry)=> { %>
      <div>
        <a
          href="/business/myentries/entry/<%- reviewEntry._id %>"
          class="noSelect"
          ><div class="uk-card uk-card-hover uk-card-default">
            <div class="uk-card-media-top entryimg">
              <div class="uk-clearfix uk-position-absolute">
                <span
                  class="
                    uk-label
                    uk-float-left
                    uk-label-danger
                    uk-text-medium
                    uk-text-bold
                  "
                  ><%- reviewEntry.typeOfPlace %></span
                >
                <span
                  class="
                    uk-label
                    uk-float-left
                    uk-label-primary
                    uk-text-medium
                    uk-text-bold
                  "
                  ><%- reviewEntry.bookingStatus %></span
                >

                <!-- <span
                  class="
                    uk-label
                    uk-float-right
                    uk-label-success
                    uk-text-medium
                    uk-text-bold
                  "
                  ></span
                > -->
              </div>
              <img
                src="<%- reviewEntry.cover %>"
                alt="<%- reviewEntry.cover %>"
              />
            </div>
            <div
              class="
                uk-card-footer
                uk-card-secondary
                uk-text-left
                uk-text-default
                uk-text-bold
              "
            >
              <%- reviewEntry.name %>
            </div>
          </div></a
        >
      </div>
      <% }) %> <% } else { %>

      <p class="uk-text-center uk-text-large info-text uk-text-warning">
        No Entries
      </p>
      <% } %>
    </div>
  </div>
</div>
